#lang racket

;1
(define a 42)

;2
(define b 14)

;;3.1
(define exp1 (+ a b))
;;3.2
(define exp2 (- a b))
;;3.3
(define exp3 (+ a (* 3 b) 7))
;;3.4
(define mediaAritmetica (/ (+ a b) 2))
;;3.5
(define mediaGeometrica (sqrt (* a b)))
;;3.6
(define mediaHarmonica (/ 2 (+ (/ 1 a) (/ 1 b))))

;;4
(define soma-medias (+ (/ (+ a b) 2) (/ 2 (+ (/ 1 a) (/ 1 b)))))

;;5
(if (equal? soma-medias 49) "teste 1 ok" "teste 1 falhou")

